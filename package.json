{
  "name": "url-migration-checker",
  "version": "1.0.0",
  "description": "CLI tool to crawl a source website and validate URL migrations to a new destination",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "url-migration-checker": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "crawl": "node dist/index.js crawl",
    "validate": "node dist/index.js validate",
    "dev": "tsc --watch"
  },
  "keywords": [
    "seo",
    "migration",
    "crawler",
    "url-checker"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "p-limit": "^6.1.0",
    "undici": "^6.19.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.0",
    "typescript": "^5.5.0"
  }
}
